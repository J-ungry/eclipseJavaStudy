package ch03.quiz.case01;

public class Student {

	private String name;
	private int age;
	private int major;
	
	public String getName() {
		return name;
	}
	public int getAge() {
		return age;
	}
	public int getMajor() {
		return major;
	}
	public void setName(String name) {
		this.name = name;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public void setMajor(int major) {
		this.major = major;
	}
	
	
}
