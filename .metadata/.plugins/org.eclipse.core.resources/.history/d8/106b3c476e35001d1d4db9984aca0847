package java_study.co.kr.joongbu.case01;

import java.awt.BorderLayout;

import javax.swing.JButton;
import javax.swing.JFrame;

public class L11Swing {

	public static void main(String[] args) {
		//JFrame Swing GUI를 제공하는 객체 
		JFrame frame = new JFrame();
		JButton btn1 = new JButton("버튼 component");
		JButton btn2 = new JButton("버튼 component2");
		frame.setLayout(null);//레이아웃을 없애면 위치나크기로 컴포ㅌ넌트의 위치를 지정하 수 있다
		btn1.setBounds(0, 0, 150, 80);
		btn2.setBounds(150, 0, 100, 100);
		frame.add(btn1);
		frame.add(btn2);
		frame.setBounds(100,100,300,100);
		frame.setVisible(true);
	}

}
