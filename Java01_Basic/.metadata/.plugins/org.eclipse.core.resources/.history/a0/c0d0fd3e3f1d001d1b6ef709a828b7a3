package ch01.ex01.quiz.case01;

public class Person {

	public static void main(String[] args) {
		Info tomy = new Info();
		
		tomy.height = 170.0;
		tomy.weight = 65.0;
		tomy.name = "Tomy";
		tomy.age = 30;
		
		System.out.printf("키가 %.1f 이고 , 몸무게가 %.1f인 남성이 있습니다 ",tomy.height,tomy.weight);
		System.out.printf("이름은 %s이고 나이는 %d세 입니다  ",tomy.name,tomy.age);
	}

}
