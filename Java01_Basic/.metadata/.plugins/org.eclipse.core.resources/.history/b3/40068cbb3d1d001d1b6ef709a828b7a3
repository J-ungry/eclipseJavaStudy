package ch01.ex01.case03;

public class CircleTest {

	public static void main(String[] args) {
		//객체 생성
		Circle mirror;
		mirror = new Circle();
		
		//객체 활용 - 객체 멤버에 접근 
		mirror.radius = 10;
		mirror.name = "손거울";
		double mirrorArea = mirror.getArea();
		
		System.out.printf("손거울의 면적은 %.1f 입니다.",mirrorArea);
	}

}
