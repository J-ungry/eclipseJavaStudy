package ch01.ex02.quiz.case06Answer;

import ch01.ex02.quiz.case06.Score;

public class ScoreMain {

	public static void main(String[] args) {

		ScoreService ss = new ScoreService();
		Score[] s = new Score[3];
		
		int cnt = 0;
		
		while(true) {
			
			int menu = ss.menu();
			
			switch(menu) {
			case 0:
				System.out.println("프로그램 종료 ");
				return;
			case 1:
				ss.input(cnt,s);
			case 2:

			case 3:

			case 4:

			default:
				System.out.println("선택된 메뉴 없음 ");
			}
		}
	}

}
