package ch02.ex02.quiz.quiz02Answer;

public class Worker {

	String workerName;
	int money;
	
	public Worker(String workerName, int money) {
		this.workerName = workerName;
		this.money = money;
	}

	public void showWorkerInfo() {
		System.out.printf("%s 님의 잔액은 %d 원 입니다. . ",workerName,money);
	}
}
