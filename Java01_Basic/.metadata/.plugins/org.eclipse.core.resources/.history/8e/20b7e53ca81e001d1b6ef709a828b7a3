package ch01.ex02.quiz.Case03Answer;

import java.util.Scanner;

public class Group {
	
	Scanner sc= new Scanner(System.in);
	
	String name ; 
	int inwon ;
	String song;
	
	public void input() {
		System.out.printf("그룹명 입력 > ");
		name = sc.next();
		System.out.printf("그룹 인원 입력 > ");
		inwon = sc.nextInt();
		System.out.printf("그룹 대표곡 입력 > ");
		song = sc.next();
	}
}
